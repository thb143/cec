<div class="page">
	<div class="pageContent">
		<div class="pageFormContent" layoutH="30">
		    <dl>
	        	 <dt>卡号：</dt>
	        	 <dd>${benefitCard.code}</dd>
		    </dl>
		    <dl>
	        	 <dt>开卡手机号码：</dt>
	        	 <dd>${benefitCard.user.mobile}</dd>
		    </dl>
		    <dl>
		         <dt>卡类编号：</dt>
		         <dd>${benefitCard.type.code}</dd>
		    </dl>
		    <dl>
		         <dt>卡类名称：</dt>
		         <dd>${benefitCard.type.name}</dd>
		    </dl>
		    <dl>
		         <dt>开卡渠道：</dt>
		         <dd>${benefitCard.channel.name}</dd>
		    </dl>
		    <dl>
		         <dt>渠道开卡订单号：</dt>
		         <dd>${benefitCard.channelOrderCode}</dd>
		    </dl>
		    <dl>
		         <dt>有效开始日期：</dt>
		         <dd>${benefitCard.startDate?date}</dd>
		    </dl>
		    <dl>
		         <dt>有效结束日期：</dt>
		         <dd>${benefitCard.endDate?date}</dd>
		    </dl>
		    <dl>
		         <dt>开卡金额：</dt>
		         <dd>${benefitCard.initAmount?string(",##0.00")}</dd>
		    </dl>
		    <dl>
		    	<dt>优惠总次数：</dt>
		    	<dd>${benefitCard.totalDiscountCount}</dd>
		    </dl>
		    <dl>
		    	<dt>续费次数：</dt>
		    	<dd>${benefitCard.rechargeCount}</dd>
		    </dl>
		    <dl>
		    	<dt>当天消费次数：</dt>
		    	<dd>${benefitCard.dailyDiscountCount}</dd>
		    </dl>
		    <dl>
		    	<dt>剩余次数：</dt>
		    	<dd>${benefitCard.availableDiscountCount}</dd>
		    </dl>
		    <dl>
		    	<dt>优惠金额：</dt>
		    	<dd>${benefitCard.discountAmount?string(",##0.00")}</dd>
		    </dl>
		    <dl>
		    	<dt>状态：</dt>
		    	<dd class="${benefitCard.status.color}">${benefitCard.status}</dd>
		    </dl>
		    <dl>
		    	<dt>开卡时间：</dt>
		    	<dd>${benefitCard.createDate?datetime}</dd>
		    </dl>
		    <dl>
		    	<dt>首次消费影院：</dt>
		    	<dd>${benefitCard.firstCinema.name}</dd>
		    </dl>
		    <dl>
		    	<dt>会员生日：</dt>
		    	<dd>${(benefitCard.birthday?string("yyyy-MM-dd"))!}</dd>
		    </dl>
		</div>
	</div>
</div>